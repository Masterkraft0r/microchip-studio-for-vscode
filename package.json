{
	"name": "atmel-studio-importer",
	"displayName": "Atmel Studio Importer",
	"description": "Importer for solutions and project files for Atmel Studio",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.56.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:atmel-studio-importer.import-solution",
		"onCommand:atmel-studio-importer.add-project"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "atmel-studio-importer.import-solution",
				"title": "Atmel Studio Importer: Import solution as workspace"
			},
			{
				"command": "atmel-studio-importer.add-project",
				"title": "Atmel Studio Importer: Add project to workspace"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.56.0",
		"@typescript-eslint/eslint-plugin": "^4.14.1",
		"@typescript-eslint/parser": "^4.14.1",
		"eslint": "^7.19.0",
		"glob": "^7.1.6",
		"mocha": "^8.2.1",
		"typescript": "^4.1.3",
		"vscode-test": "^1.5.0"
	},
	"dependencies": {
		"fast-xml-parser": "^3.19.0",
		"vs-parse": "^3.1.1"
	}
}
